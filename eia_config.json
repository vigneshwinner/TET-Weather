  "regional_mappings": {
    "padd_regions": {
      "PADD 1": "East Coast",
      "PADD 2": "Midwest",
      "PADD 3": "Gulf Coast",
      "PADD 4": "Rocky Mountain",
      "PADD 5": "West Coast"
    },
    
    "natural_gas_regions": {
      "East": ["New England", "Middle Atlantic", "South Atlantic", "East North Central", "East South Central"],
      "West": ["Mountain", "Pacific"],
      "Producing": ["West South Central", "West North Central"]
    }
  },
  
  "data_quality_checks": {
    "outlier_thresholds": {
      "inventory_pct_change": 25,
      "production_pct_change": 20,
      "utilization_min": 60,
      "utilization_max": 100
    },
    
    "missing_data_threshold": 0.1,
    
    "expected_ranges": {
      "crude_oil_inventory": [250000, 550000],
      "natural_gas_inventory": [1500, 4500],
      "gasoline_inventory": [200000, 280000],
      "refinery_utilization": [60, 100]
    }
  },
  
  "feature_engineering": {
    "rolling_windows": [4, 13, 26, 52],
    "lag_periods": [1, 2, 4, 8],
    "seasonal_adjustments": {
      "summer_driving": [5, 6, 7, 8, 9],
      "winter_heating": [11, 12, 1, 2, 3],
      "shoulder_months": [4, 10]
    }
  }
